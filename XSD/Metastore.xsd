<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" 
	    xmlns:wmsa="http://enticy.com/WAF/MetastoreSchemaAnnotations/1.0" 
	    xmlns="http://enticy.com/WAF/Metastore/1.0" 
	    elementFormDefault="qualified"
	    targetNamespace="http://enticy.com/WAF/Metastore/1.0">

	<xsd:element name="METASTORE">
		<xsd:annotation>
			<xsd:documentation>Main Metastore element</xsd:documentation>
		</xsd:annotation>
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="METASTORELANG" type="METASTORELANGType" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element name="ALTERNATEDATABASES" type="ALTERNATEDATABASESType" minOccurs="0"/>
				<xsd:element name="INCLUDE" type="INCLUDEType" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element name="TABLE" type="TABLEType" minOccurs="0" maxOccurs="unbounded">
					<!-- Keys defined for uniqueness. Need to look into xsd:unique. -->
					<xsd:key name="TableFormKey">
						<xsd:selector xpath="FORM" />
						<xsd:field xpath="@NAME"/>
					</xsd:key>
					<xsd:key name="TableSearchKey">
						<xsd:selector xpath="SEARCH" />
						<xsd:field xpath="@NAME"/>
					</xsd:key>
					<xsd:key name="TableFieldKey">
						<xsd:selector xpath="FIELD" />
						<xsd:field xpath="@NAME"/>
					</xsd:key>
					<xsd:key name="TableActionKey">
						<xsd:selector xpath="ACTION" />
						<xsd:field xpath="@NAME"/>
					</xsd:key>
				</xsd:element>
				<xsd:element name="STRINGS" type="STRINGSType" minOccurs="0"/>
				<xsd:element name="SYSTEMSTRINGS" type="SYSTEMSTRINGSType" minOccurs="0"/>
			</xsd:sequence>
			<xsd:attribute name="APPLICATION_NAME" type="xsd:string" wmsa:label="Application name"/>
			<xsd:attribute name="COMMENT" type="xsd:string" wmsa:height="4" wmsa:label="Comment">
				<xsd:annotation>
					<xsd:documentation>Human readable comment, not used in application.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="APP_PROBLEMXSLT" type="xsd:string" wmsa:label="Problem XSLT"/>
			<xsd:attribute name="APP_LOGINXSLT" type="xsd:string" wmsa:label="Login XSLT"/>
			<xsd:attribute name="CONNECTIONSTRING" type="xsd:string" wmsa:label="Connection string"/>
			<xsd:attribute name="EMAILFROMHOST" type="xsd:string" wmsa:label="Email From hostname"/>
			<xsd:attribute name="EMAILSMTPSERVER" type="xsd:string" wmsa:label="Email SMTP server"/>
			<xsd:attribute name="EMAILSMTPPORT" type="xsd:integer" wmsa:label="Email SMTP port"/>
			<xsd:attribute name="EMAILSMTPAUTHTYPE" type="EMAILSMTPAUTHTYPEType" wmsa:label="Email SMTP authentication type">
				<xsd:annotation>
					<xsd:documentation>The SMTP authentication type to use to connect to the SMTP server from which email messages are sent. This is used when a user uses the system to send a page by email.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="EMAILSMTPUSER" type="xsd:string" wmsa:label="Email SMTP user">
				<xsd:annotation>
					<xsd:documentation>The SMTP user name to use to connect to the SMTP server from which email messages are sent. This is used when a user uses the system to send a page by email.</xsd:documentation>
				</xsd:annotation>
			</xsd:attribute>
			<xsd:attribute name="EMAILSMTPPASSWORD" type="xsd:string" wmsa:label="Email SMTP password"/>
			<xsd:attribute name="EMAILSMTPUSESSL" type="xsd:boolean" wmsa:label="Email SMTP use SSL"/>
			<xsd:attribute name="BUSINESSRULESSERVER" type="xsd:string" wmsa:label="Default business rules server for all forms"/>
			<xsd:attribute name="SEARCHRULESSERVER" type="xsd:string" wmsa:label="Default search rules server for all searches"/>
			<xsd:attribute name="HTMLGENERATORSERVER" type="xsd:string" wmsa:label="Default HTML generator server for all forms and searches"/>
			<xsd:attribute name="XMLGENERATORSERVER" type="xsd:string" wmsa:label="Default XML generator server for all forms and searches"/>
			<xsd:anyAttribute/>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="METASTORELANGType">
		<xsd:attribute name="LOCALE" type="xsd:string" use="required" wmsa:label="Locale"/>
		<xsd:attribute name="APPLICATION_NAME" type="xsd:string" wmsa:label="Application name"/>
		<xsd:attribute name="CONNECTIONSTRING" type="xsd:string" wmsa:label="Connection string"/>
		<xsd:attribute name="EMAILSMTPSERVER" type="xsd:string" wmsa:label="Email SMTP server"/>
		<xsd:attribute name="EMAILFROMHOST" type="xsd:string" wmsa:label="Email From hostname"/>
	</xsd:complexType>
	<xsd:complexType name="ALTERNATEDATABASESType">
		<xsd:sequence>
			<xsd:element name="DATABASE" type="DATABASEType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="DATABASEType">
		<xsd:attribute name="CONNECTIONSTRING" type="xsd:string" wmsa:label="Connection string"/>
		<xsd:attribute name="DESCRIPTION" type="xsd:string" wmsa:label="Description"/>
	</xsd:complexType>
	<xsd:complexType name="INCLUDEType">
		<xsd:attribute name="FILENAME" type="xsd:string" use="required" wmsa:label="File name"/>
	</xsd:complexType>
	<!-- Defines possible values of EMAILSMTPAUTHTYPE) -->
	<xsd:simpleType name="EMAILSMTPAUTHTYPEType">
		<xsd:restriction base="xsd:NMTOKEN">
			<xsd:enumeration value="None">
				<xsd:annotation>
					<xsd:documentation>No SMTP authentication</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Basic">
				<xsd:annotation>
					<xsd:documentation>Basic (text) SMTP authentication.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="NTLM">
				<xsd:annotation>
					<xsd:documentation>NTLM (NT Lan Manager) SMTP authentication</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Defines STRINGS -->
	<xsd:complexType name="STRINGSType">
		<xsd:sequence>
			<xsd:element name="STRING" type="STRINGType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="STRINGType">
		<xsd:sequence>
			<xsd:element name="STRINGLANG" type="STRINGLANGType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
		<xsd:attribute name="NAME" type="xsd:string" use="required" wmsa:label="Name"/>
		<xsd:attribute name="VALUE" type="xsd:string" wmsa:label="Value"/>
	</xsd:complexType>
	<xsd:complexType name="STRINGLANGType">
		<xsd:attribute name="LOCALE" type="xsd:string" use="required" wmsa:label="Locale"/>
		<xsd:attribute name="VALUE" type="xsd:string" wmsa:label="Value"/>
	</xsd:complexType>
	<!-- Defines SYSTEMSTRINGS -->
	<xsd:complexType name="SYSTEMSTRINGSType">
		<xsd:sequence>
			<xsd:element name="SYSTEMSTRING" type="SYSTEMSTRINGType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="SYSTEMSTRINGType">
		<xsd:sequence>
			<xsd:element name="SYSTEMSTRINGLANG" type="SYSTEMSTRINGLANGType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
		<xsd:attribute name="NAME" type="xsd:string" use="required" wmsa:label="Name"/>
		<xsd:attribute name="VALUE" type="xsd:string" wmsa:label="Value"/>
	</xsd:complexType>
	<xsd:complexType name="SYSTEMSTRINGLANGType">
		<xsd:attribute name="LOCALE" type="xsd:string" use="required" wmsa:label="Locale"/>
		<xsd:attribute name="VALUE" type="xsd:string" wmsa:label="Value"/>
	</xsd:complexType>
	<!-- Defines an ACTION -->
	<xsd:complexType name="ACTIONType">
		<xsd:sequence>
			<xsd:element name="ACTIONLANG" type="ACTIONLANGType" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="SECURITY" type="ACTIONSECURITYType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
		<xsd:attribute name="NAME" type="xsd:string" use="required" wmsa:label="Name"/>
		<xsd:attribute name="COMMENT" type="xsd:string" wmsa:height="4" wmsa:label="Comment"/>
		<xsd:attribute name="LABEL" type="xsd:string" wmsa:label="Label"/>
		<xsd:attribute name="TITLE" type="xsd:string" wmsa:label="Title"/>
		<xsd:attribute name="DESCRIPTION" type="xsd:string" wmsa:label="Description"/>
		<xsd:attribute name="URL" type="xsd:string" wmsa:tab="Web" wmsa:label="URL"/>
		<xsd:attribute name="PAGE" type="xsd:string" wmsa:tab="Web" wmsa:label="Page"/>
		<xsd:attribute name="ACTION" type="ACTIONACTIONType" wmsa:tab="Web" wmsa:label="Action"/>
		<xsd:attribute name="TABLENAME" type="xsd:string" wmsa:tab="Web" wmsa:label="Table name"/>
		<xsd:attribute name="FORMNAME" type="xsd:string" wmsa:tab="Web" wmsa:label="Form name"/>
		<xsd:attribute name="SEARCHNAME" type="xsd:string" wmsa:tab="Web" wmsa:label="Search name"/>
		<xsd:attribute name="REPORTNAME" type="xsd:string" wmsa:tab="Web" wmsa:label="Report name"/>
		<xsd:attribute name="CONTENT" type="xsd:string" wmsa:tab="Web" wmsa:label="Content to display"/>
		<xsd:attribute name="FIELDNAME" type="xsd:string" wmsa:tab="Web" wmsa:label="Field name"/>
		<xsd:attribute name="NEXTACTIONNAME" type="xsd:string" wmsa:tab="Web" wmsa:label="Next action name"/>
		<xsd:attribute name="EXECACTIONNAME" type="xsd:string" wmsa:label="Execute action name"/>
		<xsd:attribute name="CONNECTIONSTRING" type="xsd:string" wmsa:label="Connection string"/>
		<xsd:attribute name="ADD_PKEY" type="xsd:boolean" wmsa:label="Add primary key as primary key?"/>
		<xsd:attribute name="ADD_FKEY" type="xsd:boolean" wmsa:label="Add primary key as foreign key?"/>
		<xsd:attribute name="ADD_PARENT_PKEY" type="xsd:boolean" wmsa:label="Add parent key as primary key?"/>
		<xsd:attribute name="ADD_PARENT_FKEY" type="xsd:boolean" wmsa:label="Add parent key as foreign key?"/>
		<xsd:attribute name="SUBMIT_EDIT_FORM" type="xsd:boolean" wmsa:tab="Web" wmsa:label="Submit edit form?"/>
		<xsd:attribute name="SUBMIT_SEARCH_FORM" type="xsd:boolean" wmsa:tab="Web" wmsa:label="Submit search form?"/>
		<xsd:attribute name="SUBMIT_SEARCH_RESULTS" type="xsd:boolean" wmsa:tab="Web" wmsa:label="Submit search results?"/>
		<xsd:attribute name="ADD_RESTORE_SEARCH_VALUES" type="xsd:boolean" wmsa:tab="Web" wmsa:label="Restore search values?"/>
		<xsd:attribute name="ADD_SEARCH_CRITERIA" type="xsd:boolean" wmsa:tab="Web" wmsa:label="Add search criteria onto URL?"/>
		<xsd:attribute name="USE_IMAGE" type="xsd:boolean" wmsa:label="Use image?"/>
		<xsd:attribute name="USE_DATABASE" type="xsd:boolean" wmsa:label="Use database?"/>
		<xsd:attribute name="DIVIDER" type="xsd:boolean" wmsa:label="Is divider?"/>
		<xsd:attribute name="TARGET_NEW" type="xsd:boolean" wmsa:tab="Web" wmsa:label="Target is new window?"/>
		<xsd:attribute name="TARGET_POPUP" type="xsd:boolean" wmsa:tab="Web" wmsa:label="Target is popup?"/>
		<xsd:attribute name="USE_URL_AS_IS" type="xsd:boolean" wmsa:tab="Web" wmsa:label="Use URL as is?"/>
		<xsd:attribute name="CONFIRM" type="xsd:boolean" wmsa:label="Confirm?"/>
		<xsd:attribute name="CONFIRMTEXT" type="xsd:string" wmsa:label="Confirm text"/>
		<xsd:attribute name="SECURECONNECTION" type="SECURECONNECTIONType" wmsa:label="Secure (SSL) connection type"/>
		<xsd:attribute name="ACCESSKEY" type="ACCESSKEYType" wmsa:tab="Web" wmsa:label="Access key"/>
		<xsd:attribute name="TABINDEX" type="TABINDEXType" wmsa:tab="Web" wmsa:label="Tab index"/>
	</xsd:complexType>
	<xsd:complexType name="ACTIONLANGType">
		<xsd:attribute name="LOCALE" type="xsd:string" use="required" wmsa:label="Locale"/>
		<xsd:attribute name="LABEL" type="xsd:string" wmsa:label="Label"/>
		<xsd:attribute name="TITLE" type="xsd:string" wmsa:label="Title"/>
		<xsd:attribute name="DESCRIPTION" type="xsd:string" wmsa:label="Description"/>
		<xsd:attribute name="URL" type="xsd:string" wmsa:label="URL"/>
		<xsd:attribute name="PAGE" type="xsd:string" wmsa:tab="Web" wmsa:label="Page"/>
		<xsd:attribute name="ACTION" type="ACTIONACTIONType" wmsa:tab="Web" wmsa:label="Action"/>
		<xsd:attribute name="TABLENAME" type="xsd:string" wmsa:tab="Web" wmsa:label="Table name"/>
		<xsd:attribute name="FORMNAME" type="xsd:string" wmsa:tab="Web" wmsa:label="Form name"/>
		<xsd:attribute name="SEARCHNAME" type="xsd:string" wmsa:tab="Web" wmsa:label="Search name"/>
		<xsd:attribute name="REPORTNAME" type="xsd:string" wmsa:tab="Web" wmsa:label="Report name"/>
		<xsd:attribute name="CONTENT" type="xsd:string" wmsa:tab="Web" wmsa:label="Content to display"/>
		<xsd:attribute name="FIELDNAME" type="xsd:string" wmsa:tab="Web" wmsa:label="Field name"/>
		<xsd:attribute name="NEXTACTIONNAME" type="xsd:string" wmsa:tab="Web" wmsa:label="Next action name"/>
		<xsd:attribute name="EXECACTIONNAME" type="xsd:string" wmsa:label="Execute action name"/>
		<xsd:attribute name="CONNECTIONSTRING" type="xsd:string" wmsa:label="Connection string"/>
		<xsd:attribute name="CONFIRMTEXT" type="xsd:string" wmsa:label="Confirm text"/>
		<xsd:attribute name="ACCESSKEY" type="ACCESSKEYType" wmsa:label="Access key"/>
		<xsd:attribute name="TABINDEX" type="TABINDEXType" wmsa:label="Tab index"/>
	</xsd:complexType>
	<!-- Defines a FIELD -->
	<xsd:complexType name="FIELDType">
		<xsd:sequence>
			<xsd:element name="FIELDLANG" type="FIELDLANGType" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="SECURITY" type="FIELDSECURITYType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
		<xsd:attribute name="NAME" type="xsd:string" use="required" wmsa:label="Name"/>
		<xsd:attribute name="COMMENT" type="xsd:string" wmsa:height="4" wmsa:label="Comment"/>
		<xsd:attribute name="LABEL" type="xsd:string" wmsa:label="Label"/>
		<xsd:attribute name="INSTRUCTION" type="xsd:string" wmsa:height="4" wmsa:label="Instruction"/>
		<xsd:attribute name="READONLYINSTRUCTION" type="xsd:string" wmsa:height="4" wmsa:label="Instruction when read-only"/>
		<xsd:attribute name="FORMATCHOICE" type="xsd:string" wmsa:label="Format choice"/>
		<xsd:attribute name="WIDTH" type="xsd:positiveInteger" wmsa:label="Width"/>
		<xsd:attribute name="DISPLAYHEIGHT" type="xsd:positiveInteger" wmsa:label="Display height"/>
		<xsd:attribute name="DISPLAYWIDTH" type="xsd:positiveInteger" wmsa:label="Display width"/>
		<xsd:attribute name="DBCOLUMNNAME" type="xsd:string" wmsa:label="DB column name"/>
		<xsd:attribute name="FIELDTYPE" type="FIELDTYPEType" wmsa:label="Field type"/>
		<xsd:attribute name="ACCESSKEY" type="ACCESSKEYType" wmsa:label="Access key"/>
		<xsd:attribute name="TABINDEX" type="TABINDEXType" wmsa:label="Tab index"/>
		<xsd:attribute name="COLSPAN" type="xsd:positiveInteger" wmsa:label="Columns spanned"/>
		<xsd:attribute name="FK_TABLE" type="xsd:string" wmsa:tab="Foreign key" wmsa:label="FKey table"/>
		<xsd:attribute name="FK_DBTABLE" type="xsd:string" wmsa:tab="Foreign key" wmsa:label="FKey database table"/>
		<xsd:attribute name="FK_DBTABLETYPE" type="DBTABLETYPEType" wmsa:tab="Foreign key" wmsa:label="FKey database table type"/>
		<xsd:attribute name="FK_PRIMARYKEYCOLNAME" type="xsd:string" wmsa:tab="Foreign key" wmsa:label="FKey primary key column name"/>
		<xsd:attribute name="FK_PRIMARYVALUECOLNAME" type="xsd:string" wmsa:tab="Foreign key" wmsa:label="FKey primary value column name"/>
		<xsd:attribute name="FK_ALTERNATEJOIN" type="xsd:string" wmsa:tab="Foreign key" wmsa:label="FKey alternate join"/>
		<xsd:attribute name="FK_ADDITIONALWHERECLAUSE" type="xsd:string" wmsa:tab="Foreign key" wmsa:label="FKey additional where clause"/>
		<xsd:attribute name="FK_ORDERCLAUSE" type="xsd:string" wmsa:tab="Foreign key" wmsa:label="FKey order clause"/>
		<xsd:attribute name="FK_DISPLAYFORM" type="xsd:string" wmsa:tab="Foreign key" wmsa:label="FKey display form"/>
		<xsd:attribute name="FK_VALIDATETYPE" type="FKVALIDATETYPEType" wmsa:tab="Foreign key" wmsa:label="FKey validate type"/>
		<xsd:attribute name="FK_NAVIGATELABEL" type="xsd:string" wmsa:tab="Foreign key" wmsa:label="FKey navigate label"/>
		<xsd:attribute name="FK_NAVIGATETITLE" type="xsd:string" wmsa:tab="Foreign key" wmsa:label="FKey navigate title"/>
		<xsd:attribute name="FK_NAVIGATEURL" type="xsd:string" wmsa:tab="Foreign key" wmsa:label="FKey navigate URL"/>
		<xsd:attribute name="FK_NAVIGATETABLE" type="xsd:string" wmsa:tab="Foreign key" wmsa:label="FKey navigate table"/>
		<xsd:attribute name="FK_NAVIGATEFORM" type="xsd:string" wmsa:tab="Foreign key" wmsa:label="FKey navigate form"/>
		<xsd:attribute name="FK_NAVIGATEACCESSKEY" type="ACCESSKEYType" wmsa:tab="Foreign key" wmsa:label="FKey navigate access key"/>
		<xsd:attribute name="FK_CHOICELABEL" type="xsd:string" wmsa:tab="Foreign key" wmsa:label="FKey choice label"/>
		<xsd:attribute name="FK_CHOICETITLE" type="xsd:string" wmsa:tab="Foreign key" wmsa:label="FKey choice title"/>
		<xsd:attribute name="FK_CHOICEURL" type="xsd:string" wmsa:tab="Foreign key" wmsa:label="FKey choice URL"/>
		<xsd:attribute name="FK_CHOICETABLE" type="xsd:string" wmsa:tab="Foreign key" wmsa:label="FKey choice table"/>
		<xsd:attribute name="FK_CHOICESEARCH" type="xsd:string" wmsa:tab="Foreign key" wmsa:label="FKey choice search"/>
		<xsd:attribute name="FK_CHOICEACCESSKEY" type="ACCESSKEYType" wmsa:tab="Foreign key" wmsa:label="FKey choice access key"/>
		<xsd:attribute name="FK_NEWLABEL" type="xsd:string" wmsa:tab="Foreign key" wmsa:label="FKey new label"/>
		<xsd:attribute name="FK_NEWTITLE" type="xsd:string" wmsa:tab="Foreign key" wmsa:label="FKey new title"/>
		<xsd:attribute name="FK_NEWURL" type="xsd:string" wmsa:tab="Foreign key" wmsa:label="FKey new URL"/>
		<xsd:attribute name="FK_NEWTABLE" type="xsd:string" wmsa:tab="Foreign key" wmsa:label="FKey new table"/>
		<xsd:attribute name="FK_NEWFORM" type="xsd:string" wmsa:tab="Foreign key" wmsa:label="FKey new form"/>
		<xsd:attribute name="FK_NEWACCESSKEY" type="ACCESSKEYType" wmsa:tab="Foreign key" wmsa:label="FKey new access key"/>
		<xsd:attribute name="FK_SUGGESTSEARCH" type="xsd:string" wmsa:tab="Foreign Key" wmsa:label="FKey suggest Search" />
		<xsd:attribute name="FK_SUGGESTURL" type="xsd:string" wmsa:tab="Foreign key" wmsa:label="FKey suggest URL" />
		<xsd:attribute name="VALIDVALUESTYPE" type="VALIDVALUESTYPEType" wmsa:tab="Valid values" wmsa:label="Valid values type"/>
		<xsd:attribute name="VALIDVALUES" type="xsd:string" wmsa:tab="Valid values" wmsa:height="4" wmsa:label="Valid values"/>
		<xsd:attribute name="DEFAULTTYPE" type="DEFAULTTYPEType" wmsa:tab="Default" wmsa:label="Default type"/>
