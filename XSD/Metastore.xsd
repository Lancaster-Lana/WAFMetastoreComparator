	</xsd:simpleType>
	<!-- Defines the possible values of the VALIDVALUESTYPE attribute (in FIELD element) -->
	<xsd:simpleType name="VALIDVALUESTYPEType">
		<xsd:restriction base="xsd:NMTOKEN">
			<xsd:enumeration value="FieldValidValuesTypeDefaultSql"/>
			<xsd:enumeration value="FieldValidValuesTypeCustomSql"/>
			<xsd:enumeration value="FieldValidValuesTypeList"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Defines the possible values of the DEFAULTTYPE attribute (in FIELD element) -->
	<xsd:simpleType name="DEFAULTTYPEType">
		<xsd:restriction base="xsd:NMTOKEN">
			<xsd:enumeration value="FieldDefaultTypeNone"/>
			<xsd:enumeration value="FieldDefaultTypeConstant"/>
			<xsd:enumeration value="FieldDefaultTypeProfile"/>
			<xsd:enumeration value="FieldDefaultTypeDefaultSql"/>
			<xsd:enumeration value="FieldDefaultTypeCustomSql"/>
			<xsd:enumeration value="FieldDefaultTypeExternal"/>
			<xsd:enumeration value="FieldDefaultTypeToday"/>
			<xsd:enumeration value="FieldDefaultTypeNow"/>
