	<!-- Defines possible values of FIELDTYPE (in FIELD element) -->
	<xsd:simpleType name="FIELDTYPEType">
		<xsd:restriction base="xsd:NMTOKEN">
			<xsd:enumeration value="FieldTypeBirthDate"/>
			<xsd:enumeration value="FieldTypeBLOB"/>
			<xsd:enumeration value="FieldTypeBoolean"/>
			<xsd:enumeration value="FieldTypeCLOB"/>
			<xsd:enumeration value="FieldTypeCurrency"/>
			<xsd:enumeration value="FieldTypeDate"/>
			<xsd:enumeration value="FieldTypeDateTime"/>
			<xsd:enumeration value="FieldTypeDivider"/>
			<xsd:enumeration value="FieldTypeDividerLabel"/>
			<xsd:enumeration value="FieldTypeEmail"/>
			<xsd:enumeration value="FieldTypeFile"/>
			<xsd:enumeration value="FieldTypeForeignKey"/>
			<xsd:enumeration value="FieldTypeHTML"/>
			<xsd:enumeration value="FieldTypeInteger"/>
			<xsd:enumeration value="FieldTypeLongVarchar"/>
			<xsd:enumeration value="FieldTypeMultiLine"/>
			<xsd:enumeration value="FieldTypeNCLOB"/>
			<xsd:enumeration value="FieldTypeNText"/>
			<xsd:enumeration value="FieldTypeNumber"/>
			<xsd:enumeration value="FieldTypeOracleLastModified"/>
			<xsd:enumeration value="FieldTypeOracleRowID"/>
			<xsd:enumeration value="FieldTypeOracleSequence"/>
			<xsd:enumeration value="FieldTypePassword"/>
			<xsd:enumeration value="FieldTypePath"/>
			<xsd:enumeration value="FieldTypePercent"/>
			<xsd:enumeration value="FieldTypePhone"/>
			<xsd:enumeration value="FieldTypeSQLServerIdentity"/>
			<xsd:enumeration value="FieldTypeSQLServerLastModified"/>
			<xsd:enumeration value="FieldTypeSQLServerTimeStamp"/>
			<xsd:enumeration value="FieldTypeSSN"/>
			<xsd:enumeration value="FieldTypeState"/>
			<xsd:enumeration value="FieldTypeTime"/>
			<xsd:enumeration value="FieldTypeText"/>
			<xsd:enumeration value="FieldTypeURL"/>
			<xsd:enumeration value="FieldTypeUserID"/>
			<xsd:enumeration value="FieldTypeUTCDateTime"/>
			<xsd:enumeration value="FieldTypeValidValues"/>
			<xsd:enumeration value="FieldTypeVarchar"/>
			<xsd:enumeration value="FieldTypeXML"/>
			<xsd:enumeration value="FieldTypeZipCode"/>
		</xsd:restriction>
