		<xsd:attribute name="SINGLE_ROW_SELECTION" type="xsd:boolean" wmsa:label="Single row selection?"/>
		<xsd:attribute name="REMEMBER_SEARCH_VALUES" type="xsd:boolean" wmsa:label="Remember search values?"/>
		<xsd:attribute name="SELECTEDTAB" type="xsd:string" wmsa:label="Selected tab"/>
		<xsd:attribute name="SEARCHSUBMISSION" type="SEARCHSUBMISSIONType" wmsa:label="Search submission"/>
		<xsd:attribute name="SECURECONNECTION" type="SECURECONNECTIONType" wmsa:label="Secure (SSL) connection type"/>
