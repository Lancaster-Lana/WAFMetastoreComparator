	<xsd:complexType name="SEARCHLANGType">
		<xsd:attribute name="LOCALE" type="xsd:string" use="required" wmsa:label="Locale"/>
		<xsd:attribute name="TITLE" type="xsd:string" wmsa:label="Title"/>
		<xsd:attribute name="INSTRUCTION" type="xsd:string" wmsa:height="4" wmsa:label="Instruction"/>
		<xsd:attribute name="RESULTINSTRUCTION" type="xsd:string" wmsa:height="4" wmsa:label="Result instruction"/>
		<xsd:attribute name="RESULTTITLE" type="xsd:string" wmsa:label="Result title"/>
		<xsd:attribute name="RESULTEMPTYMESSAGE" type="xsd:string" wmsa:label="Result empty message"/>
		<xsd:attribute name="EMAILURL" type="xsd:string" wmsa:label="Email URL"/>
		<xsd:attribute name="PRINTURL" type="xsd:string" wmsa:label="Print URL"/>
		<xsd:attribute name="CSVURL" type="xsd:string" wmsa:label="CSV URL"/>
		<xsd:attribute name="SEARCHFORMHELPURL" type="xsd:string" wmsa:label="Search form help URL"/>
		<xsd:attribute name="SEARCHRESULTSHELPURL" type="xsd:string" wmsa:label="Search results help URL"/>
		<xsd:attribute name="XSLTNAME" type="xsd:string" wmsa:label="XSLT name"/>
		<xsd:attribute name="SEARCHACTIONBARHTML" type="xsd:string" wmsa:height="4" wmsa:label="Search action bar HTML"/>
		<xsd:attribute name="RESULTACTIONBARHTML" type="xsd:string" wmsa:height="4" wmsa:label="Result action bar HTML"/>
		<xsd:attribute name="CUSTOMACTIONHTML" type="xsd:string" wmsa:height="4" wmsa:label="Custom action HTML"/>
	</xsd:complexType>
	<!-- Defines a TABLE -->
	<xsd:complexType name="TABLEType">
		<xsd:sequence>
			<xsd:element name="TABLELANG" type="TABLELANGType" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="FIELD" type="FIELDType" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="ACTION" type="ACTIONType" minOccurs="0" maxOccurs="unbounded"/>
			<xsd:element name="FORM" type="FORMType" minOccurs="0" maxOccurs="unbounded">
				<!-- Keys defined for uniqueness.  Need to look into xsd:unique. -->
				<xsd:key name="FormFieldKey">
					<xsd:selector xpath="FORMFIELD" />
					<xsd:field xpath="@NAME"/>
				</xsd:key>
				<xsd:key name="FormRowActionKey">
					<xsd:selector xpath="FORMROWACTION" />
					<xsd:field xpath="@NAME" />
				</xsd:key>
				<xsd:key name="FormMenuActionKey">
					<xsd:selector xpath="FORMMENUACTION" />
					<xsd:field xpath="@NAME"/>
				</xsd:key>
				<xsd:key name="FormTabActionKey">
					<xsd:selector xpath="FORMTABACTION" />
					<xsd:field xpath="@NAME"/>
				</xsd:key>
				<xsd:key name="LinkedRecordKey">
					<xsd:selector xpath="LINKEDFORM" />
					<xsd:field xpath="@FORMNAME"/>
					<xsd:field xpath="@TABLENAME" />
				</xsd:key>
			</xsd:element>
			<xsd:element name="SEARCH" type="SEARCHType" minOccurs="0" maxOccurs="unbounded">
				<!-- Keys defined for uniqueness.  Need to look into xsd:unique. -->
				<xsd:key name="SearchFieldKey">
					<xsd:selector xpath="SEARCHFIELD" />
					<xsd:field xpath="@NAME"/>
				</xsd:key>
				<xsd:key name="SearchRowActionKey">
					<xsd:selector xpath="SEARCHROWACTION" />
					<xsd:field xpath="@NAME" />
				</xsd:key>
				<xsd:key name="SearchMenuActionKey">
					<xsd:selector xpath="SEARCHMENUACTION" />
					<xsd:field xpath="@NAME"/>
				</xsd:key>
				<xsd:key name="SearchTabAction">
					<xsd:selector xpath="SEARCHTABACTION" />
					<xsd:field xpath="@NAME"/>
				</xsd:key>
			</xsd:element>
		</xsd:sequence>
		<xsd:attribute name="NAME" type="xsd:string" use="required" wmsa:label="Name"/>
		<xsd:attribute name="COMMENT" type="xsd:string" wmsa:height="4" wmsa:label="Comment"/>
		<xsd:attribute name="DBTABLENAME" type="xsd:string" wmsa:label="DB table name"/>
		<xsd:attribute name="PRIMARYKEYCOLNAME" type="xsd:string" wmsa:label="Primary key column name"/>
		<xsd:attribute name="PRIMARYVALUECOLNAME" type="xsd:string" wmsa:label="Primary value column name"/>
		<xsd:attribute name="DBTABLETYPE" type="DBTABLETYPEType" wmsa:label="DB table type"/>
		<xsd:attribute name="PRIMARYKEYTYPE" type="PRIMARYKEYTYPEType" wmsa:label="Primary key type"/>
		<xsd:attribute name="BUSINESSRULESSERVER" type="xsd:string" wmsa:label="Business rules server"/>
		<xsd:attribute name="SEARCHRULESSERVER" type="xsd:string" wmsa:label="Search rules server"/>
		<xsd:attribute name="HTMLGENERATORSERVER" type="xsd:string" wmsa:label="HTML generator server"/>
		<xsd:attribute name="XMLGENERATORSERVER" type="xsd:string" wmsa:label="XML generator server"/>
		<xsd:attribute name="CONNECTIONSTRING" type="xsd:string" wmsa:label="Connection string"/>
		<xsd:attribute name="ORACLESEQUENCENAME" type="xsd:string" wmsa:label="Oracle sequence name"/>
		<xsd:attribute name="ATTACHMENT" type="xsd:boolean" wmsa:label="Is an attachment?"/>
		<xsd:attribute name="READONLY" type="xsd:boolean" wmsa:label="Is read-only?"/>
		<xsd:attribute name="FK_SUGGESTSEARCH" type="xsd:string" wmsa:label="The Default Search that should be used for all fields that use this table as FK" />
	</xsd:complexType>
	<xsd:complexType name="TABLELANGType">
		<xsd:attribute name="LOCALE" type="xsd:string" use="required" wmsa:label="Locale"/>
		<xsd:attribute name="CONNECTIONSTRING" type="xsd:string" wmsa:label="Connection string"/>
	</xsd:complexType>
	<!-- Defines a LINKEDFORM -->
	<xsd:complexType name="LINKEDFORMType">
		<xsd:attribute name="TABLENAME" type="xsd:string" wmsa:label="Table name"/>
		<xsd:attribute name="FORMNAME" type="xsd:string" wmsa:label="Form name"/>
		<xsd:attribute name="FOREIGNKEYCOLUMN" type="xsd:string" wmsa:label="Foreign key column"/>
	</xsd:complexType>
	<!-- Defines SECURITY node of a SEARCH -->
	<xsd:complexType name="SEARCHSECURITYType">
		<xsd:sequence>
			<xsd:element name="PRIV" type="PRIVType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
		<xsd:attribute name="ACTION" use="required" wmsa:label="Action">
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="ActionAny"/>
					<xsd:enumeration value="ActionSearchDisplay"/>
					<xsd:enumeration value="ActionSearchExecute"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="COMMENT" type="xsd:string" wmsa:height="4" wmsa:label="Comment"/>
		<xsd:attribute name="ALLOWANY" type="xsd:boolean" wmsa:label="Always allow this action?"/>
	</xsd:complexType>
	<!-- Defines SECURITY node of a FORM -->
	<xsd:complexType name="FORMSECURITYType">
		<xsd:sequence>
			<xsd:element name="PRIV" type="PRIVType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
		<xsd:attribute name="ACTION" use="required" wmsa:label="Action">
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="ActionAny"/>
					<xsd:enumeration value="ActionRecordCreate"/>
					<xsd:enumeration value="ActionRecordRead"/>
					<xsd:enumeration value="ActionRecordUpdate"/>
					<xsd:enumeration value="ActionRecordDelete"/>
					<xsd:enumeration value="ActionRecordDisplay"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="COMMENT" type="xsd:string" wmsa:height="4" wmsa:label="Comment"/>
		<xsd:attribute name="ALLOWANY" type="xsd:boolean" wmsa:label="Always allow this action?"/>
	</xsd:complexType>
	<!-- Defines SECURITY node of a FIELD -->
	<xsd:complexType name="FIELDSECURITYType">
		<xsd:sequence>
			<xsd:element name="PRIV" type="PRIVType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
		<xsd:attribute name="ACTION" use="required" wmsa:label="Action">
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="ActionAny"/>
					<xsd:enumeration value="ActionFieldDisplay"/>
					<xsd:enumeration value="ActionFieldModify"/>
					<xsd:enumeration value="ActionFieldSearch"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="COMMENT" type="xsd:string" wmsa:height="4" wmsa:label="Comment"/>
		<xsd:attribute name="ALLOWANY" type="xsd:boolean" wmsa:label="Always allow this action?"/>
	</xsd:complexType>
	<!-- Defines SECURITY node of an ACTION -->
	<xsd:complexType name="ACTIONSECURITYType">
		<xsd:sequence>
			<xsd:element name="PRIV" type="PRIVType" minOccurs="0" maxOccurs="unbounded"/>
		</xsd:sequence>
		<xsd:attribute name="ACTION" use="required" wmsa:label="Action">
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="ActionAny"/>
					<xsd:enumeration value="ActionActionDisplay"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="COMMENT" type="xsd:string" wmsa:height="4" wmsa:label="Comment"/>
		<xsd:attribute name="ALLOWANY" type="xsd:boolean" wmsa:label="Always allow this action?"/>
	</xsd:complexType>
	<!-- Defines the PRIV node of other SECURITY nodes -->
	<xsd:complexType name="PRIVType">
		<xsd:attribute name="FIELD1" type="xsd:string" wmsa:label="Field 1"/>
		<xsd:attribute name="OP" use="required" wmsa:label="Operator">
			<xsd:simpleType>
				<xsd:restriction base="xsd:string">
					<xsd:enumeration value="OpEquals"/>
					<xsd:enumeration value="OpDoesNotEqual"/>
					<xsd:enumeration value="OpIsLessThan"/>
					<xsd:enumeration value="OpIsGreaterThan"/>
					<xsd:enumeration value="OpIsLessThanOrEqualTo"/>
					<xsd:enumeration value="OpIsGreaterThanOrEqualTo"/>
					<xsd:enumeration value="OpContains"/>
					<xsd:enumeration value="OpDoesNotContain"/>
					<xsd:enumeration value="OpIsEmpty"/>
					<xsd:enumeration value="OpIsNotEmpty"/>
					<xsd:enumeration value="OpIsInRole"/>
					<xsd:enumeration value="OpIsNotInRole"/>
					<xsd:enumeration value="OpIsAuthenticated"/>
					<xsd:enumeration value="OpIsNotAuthenticated"/>
					<xsd:enumeration value="OpHasPrivilege"/>
					<xsd:enumeration value="OpDoesNotHavePrivilege"/>
				</xsd:restriction>
			</xsd:simpleType>
		</xsd:attribute>
		<xsd:attribute name="FIELD2" type="xsd:string" wmsa:label="Field 2"/>
		<xsd:attribute name="VALUE" type="xsd:string" wmsa:label="Value"/>
	</xsd:complexType>
	<!-- Defines the possible values of the FIELDSOURCE or FIELDTARGET attributes (in FIELD element) -->
	<xsd:simpleType name="FIELDSOURCETARGETType">
		<xsd:restriction base="xsd:NMTOKEN">
			<xsd:enumeration value="FieldSourceDatabase"/>
			<xsd:enumeration value="FieldSourceProfile"/>
			<xsd:enumeration value="FieldSourceConstant"/>
			<xsd:enumeration value="FieldSourceExternal"/>
