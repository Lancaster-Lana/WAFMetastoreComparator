	<!-- Defines possible values of SEARCHTYPE (in FIELD element) -->
	<xsd:simpleType name="SEARCHTYPEType">
		<xsd:restriction base="xsd:NMTOKEN">
			<xsd:enumeration value="FieldSearchTypeEqual"/>
			<xsd:enumeration value="FieldSearchTypeBeginsWith"/>
			<xsd:enumeration value="FieldSearchTypeGreater"/>
			<xsd:enumeration value="FieldSearchTypeGreaterOrEqual"/>
			<xsd:enumeration value="FieldSearchTypeSmaller"/>
			<xsd:enumeration value="FieldSearchTypeSmallerOrEqual"/>
			<xsd:enumeration value="FieldSearchTypeBetween"/>
			<xsd:enumeration value="FieldSearchTypeSoundex"/>
			<xsd:enumeration value="FieldSearchTypeContains"/>
			<xsd:enumeration value="FieldSearchTypeSoundsOrBegins"/>
			<xsd:enumeration value="FieldSearchTypeWordBeginsWith"/>
			<xsd:enumeration value="FieldSearchTypeIn"/>
			<xsd:enumeration value="FieldSearchTypeNotIn"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Defines possible values of FKVALIDATETYPE (in FIELD element) -->
	<xsd:simpleType name="FKVALIDATETYPEType">
		<xsd:restriction base="xsd:NMTOKEN">
			<xsd:enumeration value="FieldFKValidateTypeBeginsWith"/>
			<xsd:enumeration value="FieldFKValidateTypeEqual"/>
			<xsd:enumeration value="FieldFKValidateTypeContains"/>
