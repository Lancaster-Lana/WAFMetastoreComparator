	</xsd:simpleType>
	<!-- Defines possible values of TABINDEX (in ACTION, FIELD, and FORM elements)  -->
	<xsd:simpleType name="TABINDEXType">
		<xsd:restriction base="xsd:integer">
			<xsd:minInclusive value="0"/>
			<xsd:maxInclusive value="32767"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Defines possible values of CACHEMINUTES (in FIELD element)  -->
	<xsd:simpleType name="CACHEMINUTESType">
		<xsd:restriction base="xsd:double">
			<xsd:minInclusive value="0.0"/>
			<xsd:maxInclusive value="525599.0"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Defines possible values of TABLEACTION (in FORM element) and RESULTACTION (in SEARCH element) -->
	<xsd:simpleType name="TABLEACTIONType">
		<xsd:restriction base="xsd:NMTOKEN">
			<xsd:enumeration value="NoAction"/>
			<xsd:enumeration value="SingleChoice"/>
			<xsd:enumeration value="MultiChoice"/>
			<xsd:enumeration value="Navigate"/>
			<xsd:enumeration value="Qualifier"/>
			<xsd:enumeration value="Custom"/>
			<xsd:enumeration value="PopupEdit"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Defines possible values of SEARCHFORMTYPE (in SEARCH element) -->
	<xsd:simpleType name="SEARCHFORMTYPEType">
		<xsd:restriction base="xsd:NMTOKEN">
			<xsd:enumeration value="Advanced"/>
			<xsd:enumeration value="Basic"/>
			<xsd:enumeration value="Link"/>
			<xsd:enumeration value="None"/>
			<xsd:enumeration value="Rolodex"/>
			<xsd:enumeration value="Filter"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Defines possible values of DBTABLETYPE (in TABLE element) -->
	<xsd:simpleType name="DBTABLETYPEType">
		<xsd:restriction base="xsd:NMTOKEN">
			<xsd:enumeration value="DBTableTypeTable">
				<xsd:annotation>
					<xsd:documentation>A table in the database.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="DBTableTypeView">
				<xsd:annotation>
					<xsd:documentation>A view in the database.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="DBTableTypeSQLServerTableValuedFunction">
				<xsd:annotation>
					<xsd:documentation>A table valued function in the SQL Server database.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Defines possible values of PRIMARYKEYTYPE (in TABLE element) -->
	<xsd:simpleType name="PRIMARYKEYTYPEType">
		<xsd:restriction base="xsd:NMTOKEN">
			<xsd:enumeration value="PKeyNone">
				<xsd:annotation>
					<xsd:documentation>No primary key.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PKeyCustom">
				<xsd:annotation>
					<xsd:documentation>The primary key is created by the application.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PKeySQLServerIdentity">
				<xsd:annotation>
					<xsd:documentation>The primary key is handled by SQL Server as an IDENTITY field.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PKeySQLServerTrigger">
				<xsd:annotation>
					<xsd:documentation>The primary key is created by SQL Server as a field which is set by an insert trigger on the table.  The trigger must return the primary key value of the new row.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PKeySQLServerCalculated">
				<xsd:annotation>
					<xsd:documentation>The primary key is handled by SQL Server as a field whose value is calculated when the record is inserted.  (The value is retrieved before any trigger is executed.)</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="PKeyOracleSequence">
				<xsd:annotation>
					<xsd:documentation>The primary key is handled by Oracle using a specially named SEQUENCE.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Defines possible values of FK_*ACCESSKEY (in FIELD element) -->
	<xsd:simpleType name="ACCESSKEYType">
		<xsd:restriction base="xsd:NMTOKEN">
			<xsd:enumeration value="A"/>
			<xsd:enumeration value="B"/>
			<xsd:enumeration value="C"/>
			<xsd:enumeration value="D"/>
			<xsd:enumeration value="E"/>
			<xsd:enumeration value="F"/>
			<xsd:enumeration value="G"/>
			<xsd:enumeration value="H"/>
			<xsd:enumeration value="I"/>
			<xsd:enumeration value="J"/>
			<xsd:enumeration value="K"/>
			<xsd:enumeration value="L"/>
			<xsd:enumeration value="M"/>
			<xsd:enumeration value="N"/>
			<xsd:enumeration value="O"/>
			<xsd:enumeration value="P"/>
			<xsd:enumeration value="Q"/>
			<xsd:enumeration value="R"/>
			<xsd:enumeration value="S"/>
			<xsd:enumeration value="T"/>
			<xsd:enumeration value="U"/>
			<xsd:enumeration value="V"/>
			<xsd:enumeration value="W"/>
			<xsd:enumeration value="X"/>
			<xsd:enumeration value="Y"/>
			<xsd:enumeration value="Z"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Defines possible values of RECORDDISPLAYSTYLE (in FORM element) -->
	<xsd:simpleType name="RECORDDISPLAYSTYLEType">
		<xsd:restriction base="xsd:NMTOKEN">
			<xsd:enumeration value="UseDefault">
				<xsd:annotation>
					<xsd:documentation>Display the record as a form for parent records (non-linked records) or as rows in a table (for linked records.)</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="DisplayAsForm">
				<xsd:annotation>
					<xsd:documentation>Display the linked record set as a form (or list of forms), not as rows in a table.</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="DisplayAsTable">
				<xsd:annotation>
					<xsd:documentation>Display the linked record set as rows in a table, rather than as a form (or list of forms.)</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Defines possible values of TABLECOLLAPSESTATE (in FORM element) -->
	<xsd:simpleType name="TABLECOLLAPSESTATEType">
		<xsd:restriction base="xsd:NMTOKEN">
			<xsd:enumeration value="NotCollapsible">
				<xsd:annotation>
					<xsd:documentation>Table is not collapsible</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Collapsed">
				<xsd:annotation>
					<xsd:documentation>Table is collapsible and starts collapsed</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
			<xsd:enumeration value="Expanded">
				<xsd:annotation>
					<xsd:documentation>Table is collapsible and starts expanded</xsd:documentation>
				</xsd:annotation>
			</xsd:enumeration>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Defines possible values of FORMSUBMISSION (in FORM element) -->
	<xsd:simpleType name="FORMSUBMISSIONType">
		<xsd:restriction base="xsd:NMTOKEN">
			<xsd:enumeration value="UseDefault"/>
			<xsd:enumeration value="NewFormSubmission"/>
			<xsd:enumeration value="UseParentFormSubmission"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Defines possible values of SEARCHSUBMISSION (in SEARCH element) -->
	<xsd:simpleType name="SEARCHSUBMISSIONType">
		<xsd:restriction base="xsd:NMTOKEN">
			<xsd:enumeration value="UseDefault"/>
			<xsd:enumeration value="NewSearchSubmission"/>
			<xsd:enumeration value="UseParentSearchSubmission"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Defines possible values of SEARCHSUBMISSION (in SEARCH, FORM, and ACTION elements) -->
	<xsd:simpleType name="SECURECONNECTIONType">
		<xsd:restriction base="xsd:NMTOKEN">
			<xsd:enumeration value="KeepDefault"/>
			<xsd:enumeration value="RequireSSL"/>
			<xsd:enumeration value="RequireNoSSL"/>
			<xsd:enumeration value="KeepCurrent"/>
		</xsd:restriction>
	</xsd:simpleType>
	<!-- Defines individual types of actions.  Not used directly, but combined into ACTIONACTIONType -->
	<xsd:simpleType name="SEARCHACTIONType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Query"/>
			<xsd:enumeration value="Search"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="FORMACTIONType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="New"/>
			<xsd:enumeration value="Open"/>
			<xsd:enumeration value="Save"/>
			<xsd:enumeration value="SaveNew"/>
			<xsd:enumeration value="Delete"/>
			<xsd:enumeration value="Redisplay"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="OTHERACTIONType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="Report"/>
			<xsd:enumeration value="Display"/>
			<xsd:enumeration value="GetFile"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="CUSTOMACTIONType">
		<xsd:restriction base="xsd:string"/>
	</xsd:simpleType>
	<!-- Defines suggested values of ACTION (in ACTION element) as a combination of all other action types -->
	<xsd:simpleType name="ACTIONACTIONType">
		<xsd:union memberTypes="SEARCHACTIONType FORMACTIONType OTHERACTIONType CUSTOMACTIONType"/>
	</xsd:simpleType>
</xsd:schema>
